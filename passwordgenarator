import random
import string
def simple_password():
    lower="abcdefghijklmnopqrstuvwxyz"
    upper ="ABCDEFGHIJKLMNOPQRSTUVWXYZ"
    symbol="€¥$¢^%~|•√π÷×§∆\}{][✓="
    number ="1234567890"
    all=lower+upper+symbol+number
    
    for i in range(20):
        name=input("Enter your username:")
        length=int(input("Enter your length of password:"))
        password=" ".join(random .sample(all, length))
        print("created your random password:",password)
        print("create else")
simple_password()
